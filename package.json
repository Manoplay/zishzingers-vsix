{
  "name": "zishzingers",
  "displayName": "zishzingers",
  "description": "Adds language support for Zishzingers (A#) to Visual Studio Code. It is a language used for LittleBigPlanet script development.",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/Manoplay/zishzingers-vsix"
  },
  "publisher": "Manoplay",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "zishzingers",
      "aliases": ["A#", "zishzingers"],
      "extensions": [".as"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "zishzingers",
      "scopeName": "source.zishzingers",
      "path": "./syntaxes/zishzingers.tmLanguage.json"
    }]
  }
}
